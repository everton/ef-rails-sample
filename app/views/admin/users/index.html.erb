<%= action_title 'Users' %>

<%= link_to 'New User', new_admin_user_path %>

<ol id="users">
  <% @users.each do |user| %>
  <li>
    <%= link_to user.email, edit_admin_user_path(user) %>

    <%= button_to 'Delete User', { action: 'destroy',
          id: user.id }, method: :delete %>
  </li>
  <% end %>
</ol>
